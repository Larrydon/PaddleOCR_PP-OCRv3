	"version": "0.2.0",
	"configurations": [
	  {
		"name": "Python 偵錯工具: 目前檔案",
		"type": "debugpy",
		"request": "launch",
		"program": "${file}",
		"console": "integratedTerminal"
	  },
	  {
		"name": "Python 偵錯工具: train.py",
		"type": "debugpy",
		"request": "launch",
		"program": "${workspaceFolder}/tools/train.py",
		"console": "integratedTerminal",
		//CPU
		//"args": ["-c", "configs/rec/PP-OCRv3/rec_carplate_train_cpu.yml"],
		//GPU
		"args": ["-c", "configs/rec/PP-OCRv3/rec_carplate_train_gpu.yml"],
		"cwd": "${workspaceFolder}"
	  },
	  {
		"name": "Python 偵錯工具: eval.py",
		"type": "debugpy",
		"request": "launch",
		"program": "${workspaceFolder}/tools/eval.py",
		"console": "integratedTerminal",
		"args": [
		  "-c",
		  "configs/rec/PP-OCRv3/rec_carplate_train_gpu.yml",
		  "-o",
		  "Global.pretrained_model=./output/rec_ppocr_v3_distillation/best_accuracy"
		]
	  }
	]
  }
  